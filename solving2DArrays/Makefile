.PHONY: clean All

All:
	@echo "----------Building project:[ usingPointers2D - Debug ]----------"
	@cd "usingPointers2D" && "$(MAKE)" -f  "usingPointers2D.mk"
clean:
	@echo "----------Cleaning project:[ usingPointers2D - Debug ]----------"
	@cd "usingPointers2D" && "$(MAKE)" -f  "usingPointers2D.mk" clean
